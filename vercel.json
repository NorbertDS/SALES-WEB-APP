{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/dashboard",
      "dest": "/index.html"
    },
    {
      "src": "/analytics",
      "dest": "/index.html"
    },
    {
      "src": "/sales",
      "dest": "/index.html"
    },
    {
      "src": "/products",
      "dest": "/index.html"
    },
    {
      "src": "/reports",
      "dest": "/index.html"
    },
    {
      "src": "/users",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}
